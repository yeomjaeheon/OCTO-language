//주석 테스트

def if(condition, true_function, false_function):
    return [true_function, false_function][condition]

def fibo(n):
    def final(a, b, t, end, res):
        return res

    def calcul(a, b, t, end, res):
        ret = if(t < end, calcul, final)
        return ret(b, a + b, t + 1, end, res + [b])
    
    return calcul(0, 1, 1, n, [])

def main():
    n = 10
    result = fibo(n)