func prime(n, a):
    if a > 1:
        if n % a == 0:
            return 0
        else:
            return prime(n, a - 1)
    else:
        return 1

func is_prime(n):
    if n > 1:
        return prime(n, n - 1)
    else:
        return 0

func repet(n, s):
    if n > 0:
        if is_prime(n):
            return repet(n - 1, s + n)
        return repet(n - 1, s)
    else:
        return s

func main():
    n = 100
    repet(n, 0)
